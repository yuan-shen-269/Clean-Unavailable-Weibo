{
  "name": "WeiboCleaner",
  "version": "1.0",
  "description": "Delete unavailable weibos.",
  "author": "Yuan Shen",
  "permissions": ["storage", "declarativeContent", "activeTab"],
  "background":{
    "scripts": ["background.js", "js/jquery-3.5.1.js", "crawler.js"],
    "persistent": false
  },
  "dependencies": {
    "cheerio": "^0.19.0",
    "url-parse": "^1.0.5",
    "request": "^2.65.0"
  },
  "page_action": {
    "default_title": "Delete Unavailable Weibos",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
